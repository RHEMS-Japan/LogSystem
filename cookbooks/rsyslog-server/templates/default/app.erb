## rabbitmqadmin declare queue name=<%= node.rsyslog_app.routing_key %> durable=false
<%= node.rsyslog_app.facility %>.*    action(type="omrabbitmq"
             host="<%= node.rsyslog_app.host %>"
             virtual_host="<%= node.rsyslog_app.virtual_host %>"
             user="<%= node.rsyslog_app.user %>"
             password="<%= node.rsyslog_app.password %>"
             exchange=""
             template="<%= node.rsyslog_app.template %>"
             routing_key="<%= node.rsyslog_app.routing_key %>"
             queue.timeoutenqueue="<%= node.rsyslog_app.timeoutenqueue %>"
             queue.highwatermark="<%= node.rsyslog_app.highwatermark %>"
             queue.lowwatermark="<%= node.rsyslog_app.lowwatermark %>"
             queue.discardmark="<%= node.rsyslog_app.discardmark %>"
             queue.maxdiskspace="<%= node.rsyslog_app.maxdiskspace %>"
             queue.size="<%= node.rsyslog_app.queuesize %>"
             queue.saveonshutdown="<%= node.rsyslog_app.saveonshutdown %>"
             action.resumeretrycount="<%= node.rsyslog_app.resumeretrycount %>")
